{
  "name": "@twilio-labs/svg-to-react",
  "version": "1.0.0",
  "description": "A utility to convert raw svg files into accessible and extendable React Components.",
  "author": "Twilio Inc",
  "license": "MIT",
  "main": "dist/main.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "scripts": {
    "clean": "rimraf coverage dist",
    "build": "tsc -b tsconfig.release.json",
    "build:watch": "tsc -b -w tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "convert": "./index.js convert",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@svgr/babel-plugin-add-jsx-attribute": "^4.2.0",
    "@svgr/babel-plugin-remove-jsx-attribute": "^4.2.0",
    "@svgr/cli": "^4.2.0",
    "@svgr/core": "^4.2.0",
    "@svgr/plugin-jsx": "^4.2.0",
    "@svgr/plugin-svgo": "^4.2.0",
    "prettier": "^1.16.4"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/loadable__component": "^5.6.0",
    "@types/node": "^10.12.18",
    "@types/prettier": "^1.16.1",
    "@types/promise-timeout": "^1.3.0",
    "commander": "^2.20.0",
    "inquirer": "^6.2.2",
    "jest": "~23.6.0",
    "jest-runner-prettier": "^0.2.6",
    "rimraf": "^2.6.3",
    "ts-jest": "~23.10.5",
    "tslint": "~5.12.0",
    "tslint-config-prettier": "1.17.0",
    "typescript": "^3.2.2"
  }
}
